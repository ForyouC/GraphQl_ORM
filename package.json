{
   "name": "MyProject",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/ioredis": "^4.16.2",
      "@types/jest": "^25.2.1",
      "@types/node": "^13.13.0",
      "@types/node-fetch": "^2.5.7",
      "@types/uuid": "^7.0.2",
      "@types/yup": "^0.28.0",
      "gql2ts": "^1.10.1",
      "jest": "^25.4.0",
      "nodemon": "^2.0.3",
      "start-server-and-test": "^1.11.0",
      "ts-jest": "^25.4.0",
      "ts-node": "^6.2.0",
      "tslint": "^6.1.1",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.8.3"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "bcryptjs": "^2.4.3",
      "cross-env": "^7.0.2",
      "graphql": "^15.0.0",
      "graphql-import": "^1.0.2",
      "graphql-request": "^1.8.2",
      "graphql-tools": "^5.0.0",
      "graphql-yoga": "^1.18.3",
      "ioredis": "^4.17.1",
      "merge-graphql-schemas": "^1.7.8",
      "mysql": "^2.18.1",
      "node-fetch": "^2.6.1",
      "path": "^0.12.7",
      "redis-server": "^1.2.2",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^6.5.5",
      "typeorm": "0.2.24",
      "uuid": "^7.0.3",
      "yup": "^0.28.5"
   },
   "scripts": {
      "start": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
      "test-server": "cross-env NODE_ENV=test nodemon --exec ts-node src/index.ts",
      "test": "cross-env NODE_ENV=test jest --watch",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts",
      "ci": "start-server-and-test test-server http://localhost:4000 test"
   },
   "jest": {
      "setupFiles": [
         "<rootDir>/src/testSetup/callSetup.js"
      ],
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
